<h2><%= @course.name %></h2>

<ul class="categories">
	<% @tasks.each do |task| %>
		<% if task.publish == nil || task.publish < Time.now %>
			<li>
			<span>
				<%= link_to task.title, :controller => 'tasks', :action => 'show', :id => task.id %>&nbsp;&nbsp;
			</span>
			</li>
		<% end %>
	<% end %>
</ul>